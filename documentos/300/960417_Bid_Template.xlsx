		var overflow_div = jQuery('.sellerCommunity'),
		    seller_td = jQuery(overflow_div.find('input:checked').parent());
		if (overflow_div.length) {
			jQuery.fn.scrollTo = function(elem, speed) { 
				jQuery(this).animate({
					scrollTop: jQuery(this).scrollTop() - jQuery(this).offset().top - overflow_div.height()/2 + jQuery(elem).offset().top
				}, speed == undefined ? 1000 : speed); 
				return this; 
			};
			overflow_div.scrollTo(seller_td, 0);
		}
	}
	positionSellerCommunity();
});

function getCookie(Name){ 
	var re=new RegExp(Name+"=[^;]+", "i") //construct RE to search for target name/value pair
	if (document.cookie.match(re)) //if cookie found
		return document.cookie.match(re)[0].split("=")[1] //return its value
	return null
}

function setCookie(name, value){		
	document.cookie = name + "=" + value + "; path=/"
}

